sudo apt install git
 git --version
git config --global user.name neha
git config --global user.email ahire1185@gmail.com
git init myrepo  (myrepo folder can be created into file manager
cd myrepo (change directory)
list -a
touch sample.txt
git add.
git status
git commit -m "first change"
git log
git branch "branch1"
git checkout "branch1"
touch newfile.txt
git add.
git commit -m "second change"
git status
git checkout master
git merge branch1
